{
  "name": "rbac-backend",
  "version": "1.0.0",
  "description": "Free forever backend for RBAC - PostgreSQL/Supabase optimized",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "npx sequelize-cli db:migrate",
